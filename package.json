  "private": true,
  "dependencies": {
    "axios": "^0.20.0",
    "html-react-parser": "^0.13.0",
    "markdown-to-jsx": "^7.0.0",
    "ramda": "^0.27.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-markdown": "^4.3.1",
    "react-router-dom": "^5.2.0",
    "react-router-ga": "^1.2.3",
